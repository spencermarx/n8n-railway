    {
      "id": "prepare-data",
      "name": "Prepare Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        450,
        300
      ],
      "parameters": {
        "jsCode": "const input = $input.first().json;\n\n// Safely stringify metadata\nlet metadataJson = '{}';\ntry {\n  if (input.metadata && typeof input.metadata === 'object') {\n    metadataJson = JSON.stringify(input.metadata);\n  } else if (typeof input.metadata === 'string') {\n    JSON.parse(input.metadata);\n    metadataJson = input.metadata;\n  }\n} catch (e) {\n  metadataJson = JSON.stringify({ error: 'Failed to serialize metadata' });\n}\n\n// Escape single quotes for SQL\nfunction sqlEscape(val) {\n  if (val === null || val === undefined) return 'NULL';\n  if (typeof val === 'boolean') return val ? 'TRUE' : 'FALSE';\n  if (typeof val === 'number') return String(val);\n  return \"'\" + String(val).replace(/'/g, \"''\") + \"'\";\n}\n\nconst slack_user_id = sqlEscape(input.slack_user_id || '');\nconst action = sqlEscape(input.action || 'unknown');\nconst resource = input.resource ? sqlEscape(input.resource) : 'NULL';\nconst permitted = input.permitted === true ? 'TRUE' : 'FALSE';\nconst denial_reason = input.denial_reason ? sqlEscape(input.denial_reason) : 'NULL';\nconst request_summary = input.request_summary ? sqlEscape(input.request_summary) : 'NULL';\nconst metadata = sqlEscape(metadataJson);\n\nconst sql = `INSERT INTO alfred.audit_log (slack_user_id, action, resource, permitted, denial_reason, request_summary, metadata)\nVALUES (${slack_user_id}, ${action}, ${resource}, ${permitted}, ${denial_reason}, ${request_summary}, ${metadata}::jsonb)\nRETURNING id, timestamp;`;\n\nreturn [{ json: { query: sql } }];"
      }
    },
