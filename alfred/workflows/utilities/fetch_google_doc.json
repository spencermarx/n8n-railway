    {
      "id": "format-success",
      "name": "Format Success Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2670,
        80
      ],
      "parameters": {
        "jsCode": "const doc = $input.first().json;\nconst resolvedFrom = $('Merge Auth Context').first().json.resolved_from;\n\n// Extract text content from document\nlet text = '';\nif (doc.body?.content) {\n  for (const element of doc.body.content) {\n    if (element.paragraph?.elements) {\n      for (const textElement of element.paragraph.elements) {\n        if (textElement.textRun?.content) {\n          text += textElement.textRun.content;\n        }\n      }\n    }\n  }\n}\n\nreturn [{\n  json: {\n    success: true,\n    content: text.trim(),\n    document_id: doc.documentId,\n    document_title: doc.title,\n    document_url: `https://docs.google.com/document/d/${doc.documentId}/edit`,\n    resolved_from: resolvedFrom,\n    error: null\n  }\n}];"
      }
    },
    {
      "id": "format-fetch-error",
      "name": "Format Fetch Error",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2670,
        220
      ],
      "parameters": {
        "jsCode": "const error = $input.first().json;\nconst documentId = $('Merge Auth Context').first().json.document_id;\n\nlet errorMessage = 'Failed to fetch document';\nif (error.error?.message) {\n  errorMessage = error.error.message;\n} else if (error.message) {\n  errorMessage = error.message;\n} else if (typeof error === 'string') {\n  errorMessage = error;\n}\n\nreturn [{\n  json: {\n    success: false,\n    content: null,\n    document_id: documentId,\n    document_title: null,\n    error: errorMessage\n  }\n}];"
      }
    }
