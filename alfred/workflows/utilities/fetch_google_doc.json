      "parameters": {
        "jsCode": "const input = $input.first().json;\n\n// Determine which input method was provided\nlet inputType = 'none';\nlet value = null;\n\nif (input.document_id && input.document_id.trim()) {\n  inputType = 'document_id';\n  value = input.document_id.trim();\n} else if (input.config_key && input.config_key.trim()) {\n  inputType = 'config_key';\n  value = input.config_key.trim();\n} else if (input.document_name && input.document_name.trim()) {\n  inputType = 'document_name';\n  value = input.document_name.trim();\n}\n\nreturn [{\n  json: {\n    input_type: inputType,\n    input_value: value,\n    slack_user_id: input.slack_user_id,\n    document_id: input.document_id || null,\n    config_key: input.config_key || null,\n    document_name: input.document_name || null\n  }\n}];"
      }
