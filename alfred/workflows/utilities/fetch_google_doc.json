      "id": "fetch-document",
      "name": "Fetch Document API",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        2450,
        140
      ],
      "parameters": {
        "method": "GET",
        "url": "=https://docs.googleapis.com/v1/documents/{{ $json.document_id }}",
        "sendHeaders": true,
