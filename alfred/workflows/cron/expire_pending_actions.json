  "name": "‚è∞ Cron | Expire Pending Actions",
  "description": null,
  "active": true,
  "isArchived": false,
  "nodes": [
    {
      "id": "cron-trigger",
      "name": "Every 5 Minutes",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [
        200,
        300
      ],
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 5
            }
          ]
        }
      }
    },
    {
      "id": "find-expired",
      "name": "Find Expired Actions",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [
        420,
        300
      ],
      "parameters": {
        "operation": "executeQuery",
        "query": "UPDATE alfred.pending_actions\nSET \n  status = 'expired',\n  resolved_at = NOW(),\n  resolution_note = 'Automatically expired after timeout'\nWHERE \n  status = 'pending' \n  AND expires_at < NOW()\nRETURNING \n  id,\n  action_type,\n  slack_user_id,\n  approval_channel_id,\n  approval_message_ts,\n  payload,\n  expires_at,\n  created_at;",
        "options": {}
      },
      "credentials": {
        "postgres": {
          "id": "8nTSHxonyIBczkvN",
          "name": "Postgres account"
        }
      }
    },
    {
      "id": "check-has-expired",
      "name": "Has Expired Actions?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2.2,
      "position": [
        640,
        300
      ],
      "parameters": {
        "conditions": {
          "options": {
            "version": 2,
            "leftValue": "",
            "caseSensitive": true,
            "typeValidation": "strict"
          },
          "combinator": "and",
          "conditions": [
            {
              "id": "expired-check",
              "leftValue": "={{ $json.id }}",
              "rightValue": "",
              "operator": {
                "type": "string",
                "operation": "notEmpty"
              }
            }
          ]
        }
      }
    },
    {
      "id": "loop-expired",
      "name": "Process Each Expired",
      "type": "n8n-nodes-base.splitInBatches",
      "typeVersion": 3,
      "position": [
        860,
        200
      ],
      "parameters": {
        "batchSize": 1,
        "options": {}
      }
    },
    {
      "id": "build-expired-card",
      "name": "Build Expired Card",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1080,
        200
      ],
      "parameters": {
        "jsCode": "const item = $input.first().json;\nconst payload = item.payload || {};\n\nconst blocks = [\n  {\n    type: 'header',\n    text: {\n      type: 'plain_text',\n      text: 'Email Expired',\n      emoji: true\n    }\n  },\n  {\n    type: 'section',\n    fields: [\n      {\n        type: 'mrkdwn',\n        text: `*To:*\\n${payload.to || 'Not specified'}`\n      },\n      {\n        type: 'mrkdwn',\n        text: `*Subject:*\\n${payload.subject || 'No subject'}`\n      }\n    ]\n  },\n  {\n    type: 'context',\n    elements: [\n      {\n        type: 'mrkdwn',\n        text: `Expired - no action taken. Originally requested by <@${item.slack_user_id}>`\n      }\n    ]\n  }\n];\n\nreturn [{\n  json: {\n    ...item,\n    blocks: blocks\n  }\n}];"
      }
    },
    {
      "id": "loop-done",
      "name": "Loop Complete",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [
        1520,
        200
      ],
      "parameters": {}
    },
    {
      "id": "no-expired",
      "name": "No Expired Actions",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [
        860,
        400
      ],
      "parameters": {}
    },
    {
      "id": "call-slack-service",
      "name": "Update Slack Card - Expired",
      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1.2,
      "position": [
        1300,
        200
      ],
      "parameters": {
        "workflowId": {
          "__rl": true,
          "mode": "id",
          "value": "xbFJVyUFvGfUbY4s"
        },
        "workflowInputs": {
          "mappingMode": "defineBelow",
          "value": {
            "action": "update_message",
            "channel_id": "={{ $json.approval_channel_id }}",
            "message_ts": "={{ $json.approval_message_ts }}",
            "text": "Email request expired",
            "blocks": "={{ $json.blocks }}"
          }
        },
        "options": {}
      },
      "onError": "continueRegularOutput"
    }
  ],
  "connections": {
